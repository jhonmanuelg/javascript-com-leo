// funcoes Callback01 em js.
/*
é uma função passada a outra função como argumento, que é então invocado dentro
 da função externa para completar algum tipo de rotina ou ação. O exemplo acima 
 é de uma synchronous callback, uma vez que é executada imediatamente.
*/

 const fabricante = ["jhon", "karla", "BMW"]
 function imprimir(nome, indice){
     console.log(`${indice + 1}. ${nome}`)
 }
 fabricante.forEach(imprimir)
 fabricante.forEach(fabricante => console.log(fabricante)
 )